        // Copy Gem Prompt Function
        function copyGemPrompt(promptId) {
            const promptElement = document.getElementById(promptId);
            const promptText = promptElement.textContent;

            navigator.clipboard.writeText(promptText).then(() => {
                const button = event.target;
                const originalText = button.textContent;
                button.textContent = 'âœ“ Copied!';
                button.style.background = '#10b981';

                // Track copy event
                if (typeof window.trackEvent === 'function') {
                    window.trackEvent('gem_prompt_copied', {
                        gem_type: 'Merck_RPO_Talent_Partner',
                        page_path: window.location.pathname
                    });
                }

                setTimeout(() => {
                    button.textContent = originalText;
                    button.style.background = '#4f46e5';
                }, 2000);
            }).catch(err => {
                console.error('Failed to copy:', err);
                alert('Failed to copy. Please select and copy manually.');
            });
        }

        // Track page view
        document.addEventListener('DOMContentLoaded', function() {
            if (typeof window.trackEvent === 'function') {
                window.trackEvent('use_case_page_view', {
                    use_case_name: 'Merck_RPO_Talent_Partner',
                    page_path: window.location.pathname
                });
            }
        });

        // Track CTA clicks
        document.querySelectorAll('a[href*="gbs-prompts"], a[href*="rpo-training"], a[href*="submit-content"]').forEach(link => {
            link.addEventListener('click', function() {
                if (typeof window.trackEvent === 'function') {
                    window.trackEvent('use_case_cta_clicked', {
                        cta_destination: this.getAttribute('href'),
                        cta_text: this.textContent.trim(),
                        use_case_name: 'Merck_RPO_Talent_Partner',
                        page_path: window.location.pathname
                    });
                }
            });
        });
    </script>
</body>

</html>
